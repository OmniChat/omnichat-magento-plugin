{
    "name": "omnichat/magento2",
    "description": "A Magento 2 module to integrate your magento store with OmniChat",
    "type": "magento2-module",
    "version": "1.5.2",
    "license": "MIT",
    "keywords": [
        "magento",
        "omnichat",
        "chat",
        "integration"
    ],
    "authors": [
        {
            "name": "Lucas Daniel Arcanjo",
            "email": "lucas.santos@omni.chat",
            "homepage": "https://omni.chat",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "magento/framework": "^103.0",
        "magento/module-checkout": "^100.0 || ^103.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Vendor\\OmniChat\\": ""
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
